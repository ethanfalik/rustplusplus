{
    "name": "rustplusplus",
    "version": "1.21.0",
    "description": "A NodeJS Discord Bot that uses the rustplus.js library to utilize the power of the Rust+ Companion App with additional Quality-of-Life features.",
    "main": "index.ts",
    "scripts": {
        "start": "ts-node index.ts",
        "startn": "node --no-deprecation -r ts-node/register index.ts",
        "compile": "tsc",
        "preinstall": "npx npm-force-resolutions",
        "test": "tsc --noEmit -p .",
        "lint": "npx eslint . --ext .ts",
        "gen-env": "ts-node scripts/generate-env.ts",
        "gen-icons": "ts-node scripts/generate-icons.ts",
        "unusedPhrases": "npx ts-node scripts/findUnusedPhrases.ts",
        "find-eslint-comments": "grep -r --exclude-dir=node_modules 'eslint-disable' .",
        "force-update-master": "git fetch origin && git reset --hard origin/master",
        "db:setup": "npx prisma migrate dev --name init && npx prisma generate"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/alexemanuelol/rustplusplus.git"
    },
    "author": "Alexemanuelol",
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/alexemanuelol/rustplusplus/issues"
    },
    "homepage": "https://github.com/alexemanuelol/rustplusplus#readme",
    "dependencies": {
        "@discordjs/rest": "^2.6.0",
        "@discordjs/voice": "^0.19.0",
        "@formatjs/intl": "^4.0.7",
        "@liamcottle/push-receiver": "^0.0.4",
        "@napi-rs/canvas": "^0.1.88",
        "@prisma/adapter-better-sqlite3": "^7.2.0",
        "@prisma/client": "^7.2.0",
        "@types/better-sqlite3": "^7.6.13",
        "@types/lodash": "^4.17.21",
        "axios": "^1.13.2",
        "colors": "^1.4.0",
        "discord-api-types": "^0.38.37",
        "discord.js": "^14.25.1",
        "dotenv": "^17.2.3",
        "ffmpeg-static": "^5.3.0",
        "libsodium-wrappers": "^0.7.15",
        "lodash": "^4.17.21",
        "prisma": "^7.2.0",
        "rustplus-ts": "^0.0.10",
        "translate": "^3.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3",
        "winston": "^3.19.0"
    },
    "resolutions": {
        "jpeg-js": "0.4.4",
        "protobufjs": "8.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@typescript-eslint/eslint-plugin": "^8.50.1",
        "@typescript-eslint/parser": "^8.50.1",
        "eslint": "^9.39.2",
        "globals": "^16.5.0",
        "typescript-eslint": "^8.50.1"
    }
}